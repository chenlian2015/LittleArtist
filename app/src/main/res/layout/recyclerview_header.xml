<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorPrimary"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/fl_index_view_smart_manager_message_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp750_205"
        android:layout_marginTop="@dimen/dp750_18"
        android:animateLayoutChanges="false"
        >

        <aijieli.android.adapter.InterceptViewPager
            android:id="@+id/id_vp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp750_190"
            android:layout_marginTop="@dimen/dp750_15"

            android:animateLayoutChanges="true"
            android:background="#00ffff"
            android:clipChildren="false"
            android:visibility="visible" ></aijieli.android.adapter.InterceptViewPager>
    </FrameLayout>


</LinearLayout>